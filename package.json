{
  "name": "@n8n/react-expression-editor",
  "version": "1.0.0",
  "description": "React package for n8n Expression Editor with full autocomplete and CodeMirror support",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "pnpm run dev:lib & pnpm run dev:example",
    "dev:lib": "vite build --watch --mode development",
    "dev:example": "cd example && pnpm dev",
    "build": "vite build",
    "build:types": "tsc && vite build",
    "build:check": "tsc --noEmit",
    "preview": "vite preview",
    "prepublishOnly": "pnpm run build"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "dependencies": {
    "@codemirror/autocomplete": "^6.16.0",
    "@codemirror/commands": "^6.5.0",
    "@codemirror/lang-javascript": "^6.2.2",
    "@codemirror/language": "^6.10.1",
    "@codemirror/lint": "^6.9.1",
    "@codemirror/search": "^6.5.11",
    "@codemirror/state": "^6.4.1",
    "@codemirror/view": "^6.26.3",
    "@lezer/common": "^1.3.0",
    "@lezer/highlight": "^1.2.3",
    "@lezer/lr": "^1.4.2",
    "lodash": "^4.17.21",
    "prettier": "^3.6.2"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.0",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react": "^4.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.71.1",
    "typescript": "^5.3.3",
    "vite": "^5.1.4",
    "vite-plugin-dts": "^3.7.3"
  }
}
